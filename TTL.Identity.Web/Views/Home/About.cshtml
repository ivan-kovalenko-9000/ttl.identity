@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>

<ul>
    @foreach (var item in ((System.Security.Claims.ClaimsPrincipal)User).Claims)
    {
        <li>
            <b>@item.Type</b>: 
            <span>@item.Value</span>
        </li>
    }
</ul>

